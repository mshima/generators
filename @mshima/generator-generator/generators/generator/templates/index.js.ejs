function createGenerator() {
  return class <%= className %>Generator extends require('yeoman-generator') {
    constructor(args, options, features) {
      super(args, options, features);

      if (this.options.help) {
        return;
      }

      this.checkEnvironmentVersion('<%= yeomanEnvironmentVersion %>');
    }

    get initializing() {
      return {};
    }

    get prompting() {
      return {};
    }

    get configuring() {
      return {};
    }

    get composing() {
      return {};
    }

    get default() {
      return {};
    }

    get writing() {
      return {};
    }

    get postWriting() {
      return {};
    }

    get install() {
      return {};
    }

    get end() {
      return {};
    }
  };
}

module.exports = {
  createGenerator
};
